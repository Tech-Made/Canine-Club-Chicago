<script>

    document.getElementById("joinNewsLetterBtn").addEventListener("click", function(event){
        const email = document.getElementById('newsletterEmail').value;
        axios.post('/contact', {
                email: email,
            }).then(res => {
                $("#joinNewsLetterBtn").html('Joined!');
                $("#joinNewsLetterBtn").prop("disabled", true);
            }).catch(error => {
                console.error(error);
                $("#joinNewsLetterBtn").html('Something Went Wrong!');
            });
    });

    document.getElementById("contact_form_submit").addEventListener("click", function(event){
        const name = document.getElementById('name').value;
        const phone = document.getElementById('phone').value;
        const email = document.getElementById('email').value;
        const subject = document.getElementById('subject').value;
        const message = document.getElementById('message').value;
        axios.post('/contact', {
                name: name,
                phone: phone,
                email: email,
                subject: subject,
                message: message
            }).then(res => {
                $("#contact_form_submit").html('Contacted!');
                $("#contact_form_submit").prop("disabled", true);
            }).catch(error => {
                console.error(error);
                $("#contact_form_submit").html('Something Went Wrong!');
            });
    });
</script>